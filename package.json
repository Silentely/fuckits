{
  "name": "fuckits",
  "version": "2.1.0",
  "description": "AI-powered CLI tool that converts natural language to shell commands",
  "keywords": [
    "cli",
    "ai",
    "shell",
    "bash",
    "command-line",
    "gpt",
    "openai"
  ],
  "author": "faithleysath",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Silentely/fuckits.git"
  },
  "type": "module",
  "scripts": {
    "build": "bash scripts/build.sh",
    "upload-scripts": "bash scripts/upload-scripts.sh",
    "deploy": "bash scripts/deploy.sh",
    "deploy:production": "bash scripts/deploy.sh --env production",
    "deploy:staging": "bash scripts/deploy.sh --env staging",
    "one-click-deploy": "bash scripts/one-click-deploy.sh",
    "setup": "bash scripts/setup.sh",
    "dev": "wrangler dev",
    "test": "npm run test:js && npm run test:bash",
    "test:js": "vitest run",
    "test:js:watch": "vitest watch",
    "test:js:coverage": "vitest run --coverage",
    "test:bash": "bats tests/unit/bash tests/integration",
    "test:security": "bats tests/unit/bash/security.bats tests/security",
    "test:fuzzing": "bats tests/security/fuzzing.bats",
    "test:performance": "vitest run tests/performance",
    "test:e2e": "bash tests/e2e/real-deployment.test.sh",
    "test:build": "bats tests/integration/build-deploy.bats",
    "test:all": "npm run test && npm run test:js:coverage && npm run test:performance",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "security:scan": "npm audit && snyk test || echo 'Snyk not installed, skipping...'",
    "version:check": "node -e \"console.log('Version:', require('./package.json').version)\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "wrangler": "^3.80.0",
    "vitest": "^1.0.0",
    "miniflare": "^3.0.0",
    "@cloudflare/workers-types": "^4.0.0",
    "c8": "^9.0.0",
    "bats": "^1.10.0",
    "bats-support": "^0.3.0",
    "bats-assert": "^2.1.0",
    "undici": "^6.0.0"
  }
}
